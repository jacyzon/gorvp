{
    "api.example.com": {
        "/auth": {
            "backend": "example-auth-v1",
            "comment": "auth endpoint",
            "plugins": [
                "rate-limit"
            ]
        },
        "/v1/pub": {
            "backend": "example-foo-v1",
            "comment": "public endpoint"
        },
        "/v1/semi_pub": {
            "backend": "example-foo-v1",
            "comment": "public endpoint",
            "scopes": [
                "gorvp",
                "password"
            ]
        },
        "/v1/foo": {
            "backend": "example-foo-v1",
            "comment": "foo bar endpoint",
            "plugins": [
                "jwt_proxy"
            ],
            "scopes": [
                "foo",
                "password"
            ]
        },
        "/v1/ping": {
            "backend": "example-foo-v1",
            "comment": "ping pong endpoint",
            "plugins": [
                "jwt_proxy"
            ],
            "scopes": [
                "ping",
                "password"
            ]
        },
        "*": {
            "backend": "docs.example.com",
            "comment": "route not found to document site",
            "plugins": [
                "jwt_proxy"
            ]
        }
    },
    "*": {
        "*": {
            "backend": "docs.example.com",
            "comment": "route not found to document site",
            "plugins": [
                "jwt_proxy"
            ]
        }
    }
}